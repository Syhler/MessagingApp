<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.chatroomlist.chatroom.ChatRoomActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/button3"
            android:layout_width="270dp"
            android:layout_height="306dp"
            android:layout_marginStart="70dp" android:layout_marginLeft="70dp"
            android:layout_marginEnd="71dp" android:layout_marginRight="71dp"
            android:layout_marginBottom="58dp" android:text="Button"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/test_message"/>
        <TextView
            android:id="@+id/test_message"
            android:layout_width="115dp"
            android:layout_height="54dp"
            android:layout_marginStart="148dp" android:layout_marginLeft="148dp"
            android:text="Hello"
            app:layout_constraintBottom_toTopOf="@+id/button3" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button2"/>
        <Button
            android:id="@+id/button2"
            android:layout_width="220dp"
            android:layout_height="209dp" android:layout_marginStart="95dp" android:layout_marginLeft="95dp"
            android:layout_marginBottom="3dp"
            android:text="Button" app:layout_constraintBottom_toTopOf="@+id/test_message"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>